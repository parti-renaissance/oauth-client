{
    "name": "enmarche/oauth-client",
    "description": "Provide simple client_credentials OAuth client for En-Marche OAuth server",
    "homepage": "https://github.com/EnMarche/oauth-client",
    "license": "GPL-3.0",
    "type": "library",
    "autoload": {
        "psr-4": {
            "EnMarche\\OAuthClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EnMarche\\OAuthClient\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1.3",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/security-guard": "^3.3|^4.0",
        "psr/simple-cache": "^1.0",
        "psr/http-message": "^1.0",
        "psr/log": "^1.0",
        "symfony/http-foundation": "^3.3|^4.0",
        "symfony/http-kernel": "^3.3|^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.11",
        "phpunit/phpunit": "9.4.3"
    }
}
